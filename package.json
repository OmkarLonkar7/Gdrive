{
  "name": "detoxpoc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "detox-clean": "cd android && ./gradlew clean && cd ..",
    "detox-build-ios-debug": "detox build -c ios.sim.debug",
    "detox-build-ios-release": "detox build -c ios.sim.release",
    "detox-test-ios-debug": "detox test -c ios.sim.debug",
    "detox-test-ios-release": "detox test -c ios.sim.release",
    "detox-build-android-debug": "yarn detox-clean && detox build -c android.emu.debug",
    "detox-build-android-release": "yarn detox-clean && detox build -c android.emu.release",
    "detox-test-android-debug": "detox test -c android.emu.debug",
    "detox-test-android-release": "detox test -c android.emu.release"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "detox": "^20.31.0",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
